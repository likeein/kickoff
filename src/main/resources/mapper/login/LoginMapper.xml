<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.teamcommit.kickoff.Service.LoginMapper">

    <select id="member_login" parameterType="com.teamcommit.kickoff.Do.UserDO" resultType="com.teamcommit.kickoff.Do.UserDO">
        <![CDATA[
        SELECT * FROM USER WHERE USER_ID = #{userId} AND USER_PW = #{userPw}
        ]]>
    </select>


</mapper>